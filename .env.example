# API Keys
OPENAI_API_KEY=your_openai_api_key_here

# Optional: OpenAI Organization ID
# OPENAI_ORG=your_organization_id

# Browser configuration
# Set to 'true' to run browser in headless mode (no UI)
# BROWSER_HEADLESS=true 

# Additional blocked domains (comma-separated)
# ADDITIONAL_BLOCKED_DOMAINS=example.com,another-domain.com

# Metabase credentials
METABASE_USERNAME=your_metabase_username_here
METABASE_PASSWORD=your_metabase_password_here